{
  "name": "argent-grid-demo",
  "version": "1.0.0",
  "description": "Live demo for ArgentGrid - Canvas-based high-performance Angular grid",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:gh-pages": "ng build --base-href /ArgentGrid/ --configuration production",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-gh-pages": "storybook build --output-dir storybook-static"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "argent-grid": "file:../dist",
    "exceljs": "^4.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/angular": "^10.2.13",
    "@types/exceljs": "^1.3.2",
    "storybook": "^7.6.0",
    "typescript": "~5.4.0"
  }
}
